 @model IEnumerable<RequestAction>

<partial name="sidebbar" />
<div class="home-section">
    <div class="contain">
        <div class="cont">
            <div class="container">
                @if(Model.Count() > 0){
                    @foreach(var item in Model){
                        <div class="Req-Ac">
                            <div>@item.MessageForRequester</div>
                            <div class="text-end">
                                <p>@item.ActionTaker.Name <span> @item.ActionTakeDate</span></p>
                             </div> 
                        </div>
                    }
                }else{
                    <h2 class="text-center">Not Answerd Yet</h2>
                }
            </div>

        </div>
    </div>
</div>
